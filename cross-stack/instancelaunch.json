{
  "AWSTemplateFormatVersion" : "2010-09-09",
   "Parameters": {
    "StackNameSec": {
      "Description": "Name of an active CloudFormation stack that contains the networking resources",
      "Type" : "String",
      "MinLength" : 1,
      "MaxLength" : 255,
      "Default" : "vpc-test"
    }
  },
  "Resources" : {
      "Instance" : {
      "Type" : "AWS::EC2::Instance",
      "Properties" : {
        "KeyName" : "jai-test",
        "InstanceType" : "t2.micro",
        "ImageId" : "ami-2378f540",
         "SubnetId": { "Fn::ImportValue" :  {"Fn::Sub": "${StackNameSec}-SubnetAPublic" }  },
         "SecurityGroupIds" : ["sg-a85546cf"],
        "Tags" : [
        {"Key": "Name", "Value": "Stack_Instance"}
         ]
      }
    }
      },
 "Outputs":{
"insta" : {
      "Description": " instance launched",
      "Value": {"Ref": "Instance"}
      }
      }
    }
